{
  "name": "Simple-reader",
  "version": "1.2.0",
  "description": "Modern desktop RSS reader",
  "main": "./dist/electron.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "electron": "electron ./dist/electron.js",
    "start": "npm run build && npm run electron",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "package-win": "electron-builder -w --x64 -p never && electron-builder -w --ia32 -p never",
    "package-win-ci": "electron-builder -w --x64 -p never",
    "package-mac": "electron-builder --mac --x64",
    "package-linux": "electron-builder --linux --x64 -p never"
  },
  "keywords": [],
  "author": "Evarle Zhuo",
  "license": "BSD-3-Clause",
  "repository": "github:Mrzhuo2022/Simple-reader",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@fluentui/react": "^8.125.1",
    "@types/jest": "^30.0.0",
    "@types/lovefield": "^2.1.3",
    "@types/nedb": "^1.8.9",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12",
    "electron-react-devtools": "^0.5.3",
    "electron-store": "^5.2.0",
    "electron-window-state": "^5.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "font-list": "^1.4.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^30.2.0",
    "js-md5": "^0.7.3",
    "lovefield": "^2.1.12",
    "nedb": "^1.8.0",
    "prettier": "2.3.2",
    "qrcode.react": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl-universal": "^2.2.5",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-devtools": "^3.5.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rss-parser": "^3.13.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^7.0.4",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.10.1",
    "node-polyfill-webpack-plugin": "^2.0.1"
  }
}
